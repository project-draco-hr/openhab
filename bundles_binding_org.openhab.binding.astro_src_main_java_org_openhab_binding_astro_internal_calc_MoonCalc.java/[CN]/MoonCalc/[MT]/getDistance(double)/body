{
  double t=(jd - 2451545) / 36525;
  double d=297.8502042 + 445267.11151686 * t - .00163 * t * t + t * t * t / 545868 - t * t * t* t / 113065000;
  double m=357.5291092 + 35999.0502909 * t - .0001536 * t * t + t * t * t / 24490000;
