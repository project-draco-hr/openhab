{
  if (tableId < 0 || tableId > 3 || states.length != 8) {
    throw new IllegalArgumentException();
  }
  String ret=String.format("TX%s",tableId == 0 ? "A" : tableId == 1 ? "B" : tableId == 2 ? "C" : "D");
  for (int i=0; i < 8; ++i) {
switch (states[i]) {
case ON:
      ret+="1";
    break;
case OFF:
  ret+="0";
break;
case TOGGLE:
ret+="U";
break;
case NOCHANGE:
ret+="-";
break;
default :
throw new Error();
}
}
return ret;
}
