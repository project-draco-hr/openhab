{
  logger.debug("Received HTTP PUT request at '{}' for media type '{}'.",uriInfo.getPath(),type);
  String responseType=MediaTypeHelper.getResponseMediaType(headers.getAcceptableMediaTypes(),type);
  if (responseType != null) {
    Object responseObject=responseType.equals(MediaTypeHelper.APPLICATION_X_JAVASCRIPT) ? new JSONWithPadding(updateItemConfigBean(itemname,item,false),callback) : updateItemConfigBean(itemname,item,false);
    return Response.ok(responseObject,responseType).build();
  }
 else {
    return Response.notAcceptable(null).build();
  }
}
