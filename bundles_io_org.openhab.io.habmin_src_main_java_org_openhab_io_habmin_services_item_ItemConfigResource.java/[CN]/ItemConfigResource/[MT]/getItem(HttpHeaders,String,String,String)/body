{
  logger.debug("Received HTTP GET request at '{}' for media type '{}'.",uriInfo.getPath(),type);
  String responseType=MediaTypeHelper.getResponseMediaType(headers.getAcceptableMediaTypes(),type);
  if (responseType != null) {
    Object responseObject=responseType.equals(MediaTypeHelper.APPLICATION_X_JAVASCRIPT) ? new JSONWithPadding(getItemConfigBean(itemname),callback) : getItemConfigBean(itemname);
    return Response.ok(responseObject,responseType).build();
  }
 else {
    return Response.notAcceptable(null).build();
  }
}
