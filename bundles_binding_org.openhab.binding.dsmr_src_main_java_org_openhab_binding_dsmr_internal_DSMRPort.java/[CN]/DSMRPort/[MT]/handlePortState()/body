{
switch (portState) {
case CLOSED:
    if (open()) {
      portState=PortState.AUTO_DETECT;
      autoDetectTS=System.currentTimeMillis();
    }
  break;
case AUTO_DETECT:
if ((System.currentTimeMillis() - autoDetectTS) > autoDetectTimeoutMSec) {
  switchPortSpeed();
  close();
}
break;
case OPENED:
break;
}
}
