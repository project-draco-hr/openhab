{
  if (obj == null)   return false;
  if (!obj.getClass().equals(this.getClass()))   return false;
  SerialMessage other=(SerialMessage)obj;
  if (other.messageClass != this.messageClass)   return false;
  if (other.messageType != this.messageType)   return false;
  if (other.expectedReply != this.expectedReply)   return false;
  return Arrays.equals(other.messagePayload,this.messagePayload);
}
