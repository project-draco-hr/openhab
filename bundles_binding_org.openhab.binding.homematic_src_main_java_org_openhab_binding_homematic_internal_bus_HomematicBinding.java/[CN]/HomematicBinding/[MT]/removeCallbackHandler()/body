{
  if (!isCallbackServerInitialized()) {
    return;
  }
  logger.debug("Removing callback handler.");
  try {
    ccu.getConnection().init("",createHomematicId());
    cbServer.stop();
  }
 catch (  Exception e) {
    logger.debug("Error while unregistering callback server. Will be ignored.",e);
  }
  cbServer=null;
}
