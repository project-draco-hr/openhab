{
  logger.debug("Received HTTP PUT request at '{}' for media type '{}'.",new String[]{uriInfo.getPath(),type});
  String responseType=MediaTypeHelper.getResponseMediaType(headers.getAcceptableMediaTypes(),type);
  if (responseType != null) {
    Object responseObject=responseType.equals(MediaTypeHelper.APPLICATION_X_JAVASCRIPT) ? new JSONWithPadding(saveSitemap(sitemapname,uriInfo.getBaseUriBuilder().build(),sitemap),callback) : saveSitemap(sitemapname,uriInfo.getBaseUriBuilder().build(),sitemap);
    return Response.ok(responseObject,responseType).build();
  }
 else {
    return Response.notAcceptable(null).build();
  }
}
