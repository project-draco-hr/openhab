{
  logger.debug("Received HTTP POST request at '{}' for media type '{}'.",new String[]{uriInfo.getPath(),type});
  String responseType=MediaTypeHelper.getResponseMediaType(headers.getAcceptableMediaTypes(),type);
  if (responseType != null) {
    Object responseObject=responseType.equals(MediaTypeHelper.APPLICATION_X_JAVASCRIPT) ? new JSONWithPadding(new SitemapListBean(createSitemap(sitemapname,copyName,uriInfo.getAbsolutePathBuilder().build())),callback) : new SitemapListBean(createSitemap(sitemapname,copyName,uriInfo.getAbsolutePathBuilder().build()));
    return Response.ok(responseObject,responseType).build();
  }
 else {
    return Response.notAcceptable(null).build();
  }
}
