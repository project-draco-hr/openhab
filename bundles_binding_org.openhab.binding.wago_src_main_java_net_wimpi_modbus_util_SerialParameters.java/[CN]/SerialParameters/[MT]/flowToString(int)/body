{
switch (flowcontrol) {
case SerialPort.FLOWCONTROL_NONE:
    return "none";
case SerialPort.FLOWCONTROL_XONXOFF_OUT:
  return "xon/xoff out";
case SerialPort.FLOWCONTROL_XONXOFF_IN:
return "xon/xoff in";
case SerialPort.FLOWCONTROL_RTSCTS_IN:
return "rts/cts in";
case SerialPort.FLOWCONTROL_RTSCTS_OUT:
return "rts/cts out";
default :
return "none";
}
}
