{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (uid: ");
  result.append(uid);
  result.append(", subid: ");
  result.append(subid);
  result.append(", ohid: ");
  result.append(ohid);
  result.append(", subDeviceIds: ");
  result.append(subDeviceIds);
  result.append(')');
  return result.toString();
}
