{
  for (  AstroBindingProvider provider : context.getProviders()) {
    if (provider.providesBindingFor(AstroType.SUNRISE) || provider.providesBindingFor(AstroType.SUNRISE_TIME) || provider.providesBindingFor(AstroType.NOON)|| provider.providesBindingFor(AstroType.NOON_TIME)|| provider.providesBindingFor(AstroType.SUNSET)|| provider.providesBindingFor(AstroType.SUNSET_TIME)) {
      return true;
    }
  }
  return false;
}
