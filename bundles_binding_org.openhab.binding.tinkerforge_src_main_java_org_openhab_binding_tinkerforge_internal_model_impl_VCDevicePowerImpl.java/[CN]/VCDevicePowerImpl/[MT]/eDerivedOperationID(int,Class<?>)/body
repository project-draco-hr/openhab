{
  if (baseClass == MBaseDevice.class) {
switch (baseOperationID) {
case ModelPackage.MBASE_DEVICE___INIT:
      return ModelPackage.VC_DEVICE_POWER___INIT;
case ModelPackage.MBASE_DEVICE___ENABLE:
    return ModelPackage.VC_DEVICE_POWER___ENABLE;
case ModelPackage.MBASE_DEVICE___DISABLE:
  return ModelPackage.VC_DEVICE_POWER___DISABLE;
default :
return -1;
}
}
if (baseClass == MSubDevice.class) {
switch (baseOperationID) {
default :
return -1;
}
}
if (baseClass == CallbackListener.class) {
switch (baseOperationID) {
default :
return -1;
}
}
if (baseClass == MTFConfigConsumer.class) {
switch (baseOperationID) {
default :
return -1;
}
}
return super.eDerivedOperationID(baseOperationID,baseClass);
}
