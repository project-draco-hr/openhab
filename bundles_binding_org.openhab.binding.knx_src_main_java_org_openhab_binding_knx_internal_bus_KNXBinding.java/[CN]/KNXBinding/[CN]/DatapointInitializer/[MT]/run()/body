{
  while (!interrupted) {
    if (datapointsToInitialize.size() > 0) {
      HashMap<Datapoint,Integer> clonedMap=new HashMap<Datapoint,Integer>(datapointsToInitialize);
      initializeDatapoints(clonedMap);
    }
    try {
      sleep(1000L);
    }
 catch (    InterruptedException e) {
      interrupted=true;
    }
  }
}
