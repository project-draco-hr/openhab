{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  UrtsiItemConfiguration other=(UrtsiItemConfiguration)obj;
  if (_deviceId == null) {
    if (other._deviceId != null)     return false;
  }
 else   if (!_deviceId.equals(other._deviceId))   return false;
  if (other._channel != _channel)   return false;
  return true;
}
