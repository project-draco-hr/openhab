{
  logger.debug("Update config...");
  if (properties == null) {
    logger.error("Got a null properties object!");
    return;
  }
  updateConfigSettings(properties);
  closeEntityManagerFactory();
  try {
    initializeEntityManagerFactory();
    initialized=true;
  }
 catch (  Exception e) {
    logger.error(e.getMessage());
  }
}
