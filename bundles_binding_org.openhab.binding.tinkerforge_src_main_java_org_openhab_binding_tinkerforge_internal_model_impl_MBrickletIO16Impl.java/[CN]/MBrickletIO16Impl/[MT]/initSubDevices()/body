{
  ModelFactory factory=ModelFactory.eINSTANCE;
  ArrayList<String> portList=new ArrayList<String>(2);
  portList.add("a");
  portList.add("b");
  char[] a={'a','b'};
  for (int j=0; j < a.length; j++) {
    char port=a[j];
    for (int i=0; i < 8; i++) {
      DigitalSensor sensor=factory.createDigitalSensor();
      sensor.setUid(getUid());
      String subId="in" + port + String.valueOf(i);
      sensor.setSubId(subId);
      sensor.setPin(i);
      sensor.setPort(port);
      sensor.init();
      sensor.setMbrick(this);
      logger.debug("{} addSubDevice {}",LoggerConstants.TFINIT,subId);
    }
    for (int i=0; i < 8; i++) {
      DigitalActor actor=factory.createDigitalActor();
      actor.setUid(getUid());
      String subId="out" + port + String.valueOf(i);
      actor.setSubId(subId);
      actor.setPin(i);
      actor.setPort(port);
      actor.init();
      actor.setMbrick(this);
      logger.debug("{} addSubDevice {}",LoggerConstants.TFINIT,subId);
    }
  }
}
