{
  if (that instanceof LightwaveRfOnOffCommand) {
    return Objects.equals(this.messageId,((LightwaveRfOnOffCommand)that).messageId) && Objects.equals(this.roomId,((LightwaveRfOnOffCommand)that).roomId) && Objects.equals(this.deviceId,((LightwaveRfOnOffCommand)that).deviceId)&& Objects.equals(this.on,((LightwaveRfOnOffCommand)that).on);
