{
  String frame=null;
  String address[]=null;
  String who=null;
  String what=null;
  String where=null;
  who=c.getProperty("who");
  what=c.getProperty("what");
  address=c.getProperty("address").split("\\*");
  where=address[0];
  frame="*" + who + "*"+ what+ "*"+ where+ "##";
  return (frame);
}
