{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  CalDavNextEventConfig other=(CalDavNextEventConfig)obj;
  if (itemName == null) {
    if (other.itemName != null)     return false;
  }
 else   if (!itemName.equals(other.itemName))   return false;
  if (itemNameToListenTo == null) {
    if (other.itemNameToListenTo != null)     return false;
  }
 else   if (!itemNameToListenTo.equals(other.itemNameToListenTo))   return false;
  if (type != other.type)   return false;
  return true;
}
