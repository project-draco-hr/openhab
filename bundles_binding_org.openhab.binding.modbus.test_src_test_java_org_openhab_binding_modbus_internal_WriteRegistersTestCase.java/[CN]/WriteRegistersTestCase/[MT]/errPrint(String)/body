{
  System.err.println(String.format("%s (%s): registerInitialValues=%s, nonZeroOffset=%s, type=%s, valueType=%s, itemIndex=%d, command=%s, expectedValue=%s, expectingAssertionError=%s",description,serverType,Arrays.toString(registerInitialValues),nonZeroOffset,type,valueType,itemIndex,command,Arrays.toString(expectedValue),expectingAssertionError));
