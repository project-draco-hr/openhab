{
  final Map<String,MeasurementRequest> requests=new HashMap<String,MeasurementRequest>();
  for (  final NetatmoBindingProvider provider : this.providers) {
    for (    final String itemName : provider.getItemNames()) {
      final NetatmoMeasureType measureType=provider.getMeasureType(itemName);
switch (measureType) {
case TEMPERATURE:
case CO2:
case HUMIDITY:
case NOISE:
case PRESSURE:
case RAIN:
case MIN_TEMP:
case MAX_TEMP:
case MIN_HUM:
case MAX_HUM:
case MIN_PRESSURE:
case MAX_PRESSURE:
case MIN_NOISE:
case MAX_NOISE:
case MIN_CO2:
case MAX_CO2:
case SUM_RAIN:
case DATE_MIN_TEMP:
case DATE_MAX_TEMP:
case DATE_MIN_HUM:
case DATE_MAX_HUM:
case DATE_MIN_PRESSURE:
case DATE_MAX_PRESSURE:
case DATE_MIN_NOISE:
case DATE_MAX_NOISE:
case DATE_MIN_CO2:
case DATE_MAX_CO2:
case WINDSTRENGTH:
case WINDANGLE:
case GUSTSTRENGTH:
case GUSTANGLE:
case DATE_MAX_GUST:
        final NetatmoScale scale=provider.getNetatmoScale(itemName);
      addMeasurement(requests,provider,itemName,measureType,scale);
    break;
default :
  break;
}
}
}
return requests.values();
}
