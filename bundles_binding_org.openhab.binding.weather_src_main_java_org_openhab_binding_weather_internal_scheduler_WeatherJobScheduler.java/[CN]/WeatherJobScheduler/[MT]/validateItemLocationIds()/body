{
  new ItemIterator().iterate(new ItemIteratorCallback(){
    @Override public void next(    WeatherBindingConfig bindingConfig,    Item item){
      if (context.getConfig().getLocationConfig(bindingConfig.getLocationId()) == null) {
        throw new RuntimeException("Unknown locationId in item '" + item.getName() + "' with binding "+ bindingConfig);
      }
    }
  }
);
}
