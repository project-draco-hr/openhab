{
  if (!(o instanceof ConnectionSettings)) {
    return false;
  }
  ConnectionSettings other=(ConnectionSettings)o;
  return this.id.equals(other.id) && this.address.equals(other.address) && this.port == other.port && this.username.equals(other.username) && this.password.equals(other.password) && this.dimMode == other.dimMode && this.statusMode == other.statusMode && this.timeoutMSec == other.timeoutMSec;
}
