{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  SceneType other=(SceneType)obj;
  if (mode == null) {
    if (other.mode != null)     return false;
  }
 else   if (!mode.equals(other.mode))   return false;
  if (sceneNumber == null) {
    if (other.sceneNumber != null)     return false;
  }
 else   if (!sceneNumber.equals(other.sceneNumber))   return false;
  return true;
}
