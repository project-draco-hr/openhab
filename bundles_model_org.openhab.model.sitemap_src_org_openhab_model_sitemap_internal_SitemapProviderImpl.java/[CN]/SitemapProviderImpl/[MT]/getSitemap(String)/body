{
  if (modelRepo != null) {
    Sitemap sitemap=(Sitemap)modelRepo.getModel(sitemapName + ".sitemap");
    if (sitemap != null) {
      return sitemap;
    }
 else {
      logger.debug("Sitemap {} can not be found",sitemapName);
      return null;
    }
  }
 else {
    logger.debug("No model repository service is available");
    return null;
  }
}
