{
  String sOHState=StateTraslator.statesSoulissToOH(this.getsItemTypeSetpointValue(),this.getType(),(short)this.getState());
  if (sOHState == null) {
    if (!Float.isNaN(this.getState())) {
      if (this.getSetpointValue() != null)       return DecimalType.valueOf(Float.toString(this.getSetpointValue()));
 else       return null;
    }
 else     return null;
  }
 else   return DecimalType.valueOf(sOHState);
}
