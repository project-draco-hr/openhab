{
  repo=HABminApplication.getModelRepository();
  if (repo == null)   return;
  modelFile=new String(service + ".persist");
  models=(PersistenceModel)repo.getModel(modelFile);
  if (models == null) {
    logger.error("Can't open model file '{}'",service);
    return;
  }
  serviceName=new String(service);
}
