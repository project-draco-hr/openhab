{
  try {
    BindingConfig bindingConfig=Iterables.find(Iterables.concat(bindingConfigs.values()),new Predicate<BindingConfig>(){
      public boolean apply(      BindingConfig input){
        return input.itemName.equals(itemName) && KNXCoreTypeMapper.toTypeClass(input.dpt.getID()).equals(typeClass);
      }
    }
);
    return bindingConfig.datapoint;
  }
 catch (  NoSuchElementException e) {
    return null;
  }
}
