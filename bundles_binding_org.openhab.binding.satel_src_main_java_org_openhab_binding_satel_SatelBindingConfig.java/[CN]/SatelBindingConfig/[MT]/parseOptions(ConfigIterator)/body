{
  Map<String,String> options=new HashMap<String,String>();
  if (iterator.hasNext()) {
    for (    String option : iterator.next().split(",")) {
      if (option.contains("=")) {
        String[] keyVal=option.split("=",2);
        options.put(keyVal[0].toUpperCase(),keyVal[1]);
      }
 else {
        options.put(option,"");
      }
    }
    if (iterator.hasNext()) {
      throw new BindingConfigParseException(String.format("Too many elements: %s",iterator.getBindingConfig()));
    }
  }
  return options;
}
