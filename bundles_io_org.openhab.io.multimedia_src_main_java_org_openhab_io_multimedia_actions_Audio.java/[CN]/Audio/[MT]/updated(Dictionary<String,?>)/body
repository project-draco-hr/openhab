{
  if (config != null) {
    String osTTSString=(String)config.get("ostts");
    if (isNotBlank(osTTSString)) {
      try {
        Audio.useOSTTS=Boolean.valueOf(osTTSString);
      }
 catch (      IllegalArgumentException iae) {
        logger.warn("couldn't parse '{}' to a boolean");
      }
    }
  }
}
