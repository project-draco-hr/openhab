{
  if (onValue != null && offValue != null) {
    if (value == onValue) {
      return onState;
    }
 else     if (value == offValue) {
      return offState;
    }
 else {
      return UnDefType.UNDEF;
    }
  }
 else   if (onValue != null) {
    if (value == onValue) {
      return onState;
    }
 else {
      return offState;
    }
  }
 else   if (offValue != null) {
    if (value == offValue) {
      return offState;
    }
 else {
      return onState;
    }
  }
 else {
    if (value == 0) {
      return offState;
    }
 else {
      return onState;
    }
  }
}
