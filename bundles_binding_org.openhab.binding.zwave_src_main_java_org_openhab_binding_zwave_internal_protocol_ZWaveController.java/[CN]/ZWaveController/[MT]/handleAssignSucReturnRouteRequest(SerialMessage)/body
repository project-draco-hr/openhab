{
  int nodeId=lastSentMessage.getMessagePayloadByte(0);
  logger.debug("NODE {}: Got AssignSucReturnRoute request.",nodeId);
  if (incomingMessage.getMessagePayloadByte(1) != 0x00) {
    logger.error("NODE {}: Assign SUC return routes failed with error 0x{}.",nodeId,Integer.toHexString(incomingMessage.getMessagePayloadByte(0)));
    notifyEventListeners(new ZWaveNetworkEvent(ZWaveNetworkEvent.Type.AssignSucReturnRoute,nodeId,ZWaveNetworkEvent.State.Failure));
  }
 else {
    notifyEventListeners(new ZWaveNetworkEvent(ZWaveNetworkEvent.Type.AssignSucReturnRoute,nodeId,ZWaveNetworkEvent.State.Success));
  }
}
