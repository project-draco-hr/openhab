{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (cmd: ");
  result.append(cmd);
  result.append(", label: ");
  result.append(label);
  result.append(", icon: ");
  result.append(icon);
  result.append(')');
  return result.toString();
}
