{
  if (obj instanceof RFXComLighting1Message)   return convertLighting1ToState((RFXComLighting1Message)obj,valueSelector);
 else   if (obj instanceof RFXComLighting2Message)   return convertLighting2ToState((RFXComLighting2Message)obj,valueSelector);
 else   if (obj instanceof RFXComTemperatureHumidityMessage)   return convertTemperatureHumidity2ToState((RFXComTemperatureHumidityMessage)obj,valueSelector);
 else   if (obj instanceof RFXComEnergyMessage)   return convertEnergyToState((RFXComEnergyMessage)obj,valueSelector);
 else   if (obj instanceof RFXComCurtain1Message)   return convertCurtain1ToState((RFXComCurtain1Message)obj,valueSelector);
  throw new NumberFormatException("Can't convert " + obj.getClass() + " to "+ valueSelector.getItemClass());
}
