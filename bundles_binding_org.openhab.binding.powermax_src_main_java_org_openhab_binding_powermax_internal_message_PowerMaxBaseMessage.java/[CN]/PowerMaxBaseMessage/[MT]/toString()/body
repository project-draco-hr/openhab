{
  String str="\n - Raw data = " + DatatypeConverter.printHexBinary(rawData);
  str+="\n - type = " + String.format("%02X",code);
  if (sendType != null) {
    str+=" ( " + sendType.toString() + " )";
  }
 else   if (receiveType != null) {
    str+=" ( " + receiveType.toString() + " )";
  }
  return str;
}
