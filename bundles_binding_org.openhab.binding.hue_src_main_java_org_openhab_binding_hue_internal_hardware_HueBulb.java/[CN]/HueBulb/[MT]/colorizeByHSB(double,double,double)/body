{
  int newHueCalculated=new Long(Math.round(newHue * 360.0 * 182.0)).intValue();
  int newSaturationCalculated=new Long(Math.round(newSaturation * MAX_SATURATION)).intValue();
  int newBrightnessCalculated=new Long(Math.round(newBrightness * MAX_BRIGHTNESS)).intValue();
  colorizeByHSBInternally(newHueCalculated,newSaturationCalculated,newBrightnessCalculated);
}
