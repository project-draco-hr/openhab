{
  if (event.getEventType() == SerialPortEvent.DATA_AVAILABLE) {
    try {
      String data=br.readLine();
      if (!"EOB".equals(data)) {
        log.debug("Received message from CUL: " + data);
        notifyDataReceived(data);
      }
    }
 catch (    IOException e) {
      notifyError(e);
    }
  }
}
