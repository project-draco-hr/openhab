{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  UCPRelayConfig other=(UCPRelayConfig)obj;
  if (boardName == null) {
    if (other.boardName != null)     return false;
  }
 else   if (!boardName.equals(other.boardName))   return false;
  if (relayNumber != other.relayNumber)   return false;
  return true;
}
