{
switch (input.toUpperCase()) {
case "LCN":
    return NATIVE;
case "CELSIUS":
case "???CELSIUS":
case "???C":
  return CELSIUS;
case "KELVIN":
case "???KELVIN":
case "???K":
return KELVIN;
case "FAHRENHEIT":
case "???FAHRENHEIT":
case "???F":
return FAHRENHEIT;
case "LUX_T":
case "LX_T":
return LUX_T;
case "LUX":
case "LX":
return LUX_I;
case "M/S":
return METERPERSECOND;
case "%":
return PERCENT;
case "PPM":
return PPM;
case "VOLT":
case "V":
return VOLT;
case "AMPERE":
case "AMP":
case "A":
return AMPERE;
case "DEGREE":
case "???":
return DEGREE;
default :
throw new IllegalArgumentException();
}
}
