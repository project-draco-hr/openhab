{
switch (unit) {
case NATIVE:
    return fromNative((int)v);
case CELSIUS:
  return fromCelsius(v,abs);
case KELVIN:
return fromKelvin(v,abs);
case FAHRENHEIT:
return fromFahrenheit(v,abs);
case LUX_T:
return fromLuxT(v);
case LUX_I:
return fromLuxI(v);
case METERPERSECOND:
return fromMetersPerSec(v);
case PERCENT:
return fromPercent(v);
case PPM:
return fromPpm(v);
case VOLT:
return fromVolt(v);
case AMPERE:
return fromAmpere(v);
case DEGREE:
return fromDegree(v);
}
throw new Error();
}
