{
  if (obj == null || !(obj instanceof ForecastBindingConfig)) {
    return false;
  }
  ForecastBindingConfig comp=(ForecastBindingConfig)obj;
  return new EqualsBuilder().append(getLocationId(),comp.getLocationId()).append(getType(),comp.getType()).append(getProperty(),comp.getProperty()).append(forecastDay,comp.getForecastDay()).isEquals();
}
