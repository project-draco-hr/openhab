{
  if (v.length != 2 || k.length != 4)   throw new Exception("Invalid length for parameter v or k");
  int v0=v[0], v1=v[1], sum=0;
  for (int i=0; i < num_cycles; i++) {
    v0+=(((v1 << 4) ^ (v1 >> 5)) + v1) ^ (sum + k[sum & 3]);
    sum+=delta;
    v1+=(((v0 << 4) ^ (v0 >> 5)) + v0) ^ (sum + k[(sum >> 11) & 3]);
  }
  v[0]=v0;
  v[1]=v1;
}
