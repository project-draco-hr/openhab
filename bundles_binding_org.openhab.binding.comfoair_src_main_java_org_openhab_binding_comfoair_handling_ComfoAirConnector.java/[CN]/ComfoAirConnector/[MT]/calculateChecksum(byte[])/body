{
  int datasum=0;
  for (int i=0; i < block.length; i++) {
    datasum+=(int)block[i];
  }
  datasum+=173;
  String hexString=Integer.toHexString(datasum);
  if (hexString.length() > 2) {
    hexString=hexString.substring(hexString.length() - 2);
  }
  return (byte)Integer.parseInt(hexString,16);
}
