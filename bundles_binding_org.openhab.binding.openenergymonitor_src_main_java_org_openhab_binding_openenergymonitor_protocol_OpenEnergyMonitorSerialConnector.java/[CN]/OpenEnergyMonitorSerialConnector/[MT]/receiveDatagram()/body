{
  if (in == null) {
    connect();
  }
  byte[] buffer=new byte[1024];
  int len=-1;
  try {
    while ((len=this.in.read(buffer)) > 0) {
      for (int i=0; i < len; i++) {
      }
    }
  }
 catch (  IOException e) {
    throw new OpenEnergyMonitorException("Error occured while receiving data",e);
  }
  return null;
}
