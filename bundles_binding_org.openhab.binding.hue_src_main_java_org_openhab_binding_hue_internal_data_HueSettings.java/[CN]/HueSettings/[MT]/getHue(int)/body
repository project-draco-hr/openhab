{
  if (settingsData == null) {
    logger.error("Hue bridge settings not initialized correctly.");
    return 0;
  }
  Object hue=settingsData.node("lights").node(Integer.toString(deviceNumber)).node("state").value("hue");
  if (hue instanceof Integer) {
    return (Integer)hue;
  }
 else {
    return 0;
  }
}
