{
  if (settingsData == null) {
    logger.error("Hue bridge settings not initialized correctly.");
    return 154;
  }
  Object ct=settingsData.node("lights").node(Integer.toString(deviceNumber)).node("state").value("ct");
  if (ct instanceof Integer) {
    return (Integer)ct;
  }
 else {
    return 154;
  }
}
