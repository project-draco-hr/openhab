{
  if (settingsData == null) {
    logger.error("Hue bridge settings not initialized correctly.");
    return 0;
  }
  Object sat=settingsData.node("lights").node(Integer.toString(deviceNumber)).node("state").value("sat");
  if (sat instanceof Integer) {
    return (Integer)sat;
  }
 else {
    return 0;
  }
}
