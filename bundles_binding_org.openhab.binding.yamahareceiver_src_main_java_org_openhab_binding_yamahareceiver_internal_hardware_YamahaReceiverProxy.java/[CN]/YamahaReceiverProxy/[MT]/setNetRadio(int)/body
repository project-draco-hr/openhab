{
  postAndGetResponse("<?xml version=\"1.0\" encoding=\"utf-8\"?><YAMAHA_AV cmd=\"PUT\"><NET_RADIO><List_Control><Cursor>" + "Back to Home" + "</Cursor></List_Control></NET_RADIO></YAMAHA_AV>");
  while (lineNo > 8) {
    postAndGetResponse("<?xml version=\"1.0\" encoding=\"utf-8\"?><YAMAHA_AV cmd=\"PUT\"><NET_RADIO><List_Control><Cursor>" + "Down" + "</Cursor></List_Control></NET_RADIO></YAMAHA_AV>");
    lineNo-=8;
  }
  postAndGetResponse("<?xml version=\"1.0\" encoding=\"utf-8\"?><YAMAHA_AV cmd=\"PUT\"><NET_RADIO><List_Control><Direct_Sel>" + "Line_" + lineNo + "</Direct_Sel></List_Control></NET_RADIO></YAMAHA_AV>");
}
