{
  Short oldVoltageConversionTime=voltageConversionTime;
  voltageConversionTime=newVoltageConversionTime;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,ModelPackage.MBRICKLET_VOLTAGE_CURRENT__VOLTAGE_CONVERSION_TIME,oldVoltageConversionTime,voltageConversionTime));
}
