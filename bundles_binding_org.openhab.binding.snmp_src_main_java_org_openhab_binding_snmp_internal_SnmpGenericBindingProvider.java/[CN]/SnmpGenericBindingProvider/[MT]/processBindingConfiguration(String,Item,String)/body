{
  if (item instanceof StringItem) {
    super.processBindingConfiguration(context,item,bindingConfig);
    if (bindingConfig != null) {
      SnmpBindingConfig config=parseBindingConfig(item,bindingConfig);
      addBindingConfig(item,config);
    }
 else {
      logger.warn("bindingConfig is NULL (item=" + item + ") -> processing bindingConfig aborted!");
    }
  }
 else {
    throw new BindingConfigParseException("item '" + item.getName() + "' is of type '"+ item.getClass().getSimpleName()+ "', only StringItems are allowed - please check your *.items configuration");
  }
}
