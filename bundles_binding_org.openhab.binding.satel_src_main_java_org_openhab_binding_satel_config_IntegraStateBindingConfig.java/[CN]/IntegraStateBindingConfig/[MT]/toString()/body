{
  StringBuilder sb=new StringBuilder();
  for (  int i : this.objectNumbers) {
    if (sb.length() > 0)     sb.append(",");
    sb.append(Integer.toString(i));
  }
  return String.format("IntegraStateBindingConfig: object = %s, state = %s, object nbr = %s, options = %s",this.stateType.getObjectType(),this.stateType,sb.toString(),this.options);
}
