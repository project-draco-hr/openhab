{
switch (featureID) {
case ModelPackage.REMOTE_SWITCH_ACONFIGURATION__HOUSE_CODE:
    setHouseCode(HOUSE_CODE_EDEFAULT);
  return;
case ModelPackage.REMOTE_SWITCH_ACONFIGURATION__RECEIVER_CODE:
