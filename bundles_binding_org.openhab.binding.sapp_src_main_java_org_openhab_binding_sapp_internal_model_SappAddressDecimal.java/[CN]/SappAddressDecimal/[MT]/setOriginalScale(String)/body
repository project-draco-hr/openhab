{
  if (subAddress.equals("*")) {
    originalMinScale=0;
    originalMaxScale=0xFFFF;
  }
 else   if (subAddress.equals("L")) {
    originalMinScale=0;
    originalMaxScale=0x00FF;
  }
 else   if (subAddress.equals("H")) {
    originalMinScale=0;
    originalMaxScale=0x00FF;
  }
 else {
    originalMinScale=0;
    originalMaxScale=0x0001;
  }
}
