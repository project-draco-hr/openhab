{
  short oldDistanceAverageLength=distanceAverageLength;
  distanceAverageLength=newDistanceAverageLength;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,ModelPackage.LASER_RANGE_FINDER_CONFIGURATION__DISTANCE_AVERAGE_LENGTH,oldDistanceAverageLength,distanceAverageLength));
}
