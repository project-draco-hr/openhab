{
  short oldVelocityAverageLength=velocityAverageLength;
  velocityAverageLength=newVelocityAverageLength;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,ModelPackage.LASER_RANGE_FINDER_CONFIGURATION__VELOCITY_AVERAGE_LENGTH,oldVelocityAverageLength,velocityAverageLength));
}
