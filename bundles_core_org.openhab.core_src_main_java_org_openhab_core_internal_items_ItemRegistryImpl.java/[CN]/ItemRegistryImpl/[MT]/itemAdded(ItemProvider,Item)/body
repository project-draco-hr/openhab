{
  Collection<Item> items;
synchronized (itemMap) {
    items=itemMap.get(provider);
  }
  if (items != null) {
    if (initializeItem(item)) {
      items.add(item);
    }
 else {
      return;
    }
  }
  for (  ItemRegistryChangeListener listener : listeners) {
    listener.itemAdded(item);
  }
}
