{
  if (buttonChangedButtonNum == buttonNum) {
    if (switchState == SwitchState.OFF) {
      setSwitchState(SwitchState.ON);
      logger.debug("set switch state on");
    }
 else     if (switchState == SwitchState.ON) {
      setSwitchState(SwitchState.OFF);
      logger.debug("set switch state on");
    }
 else {
      setSwitchState(SwitchState.ON);
      logger.debug("set switch state on");
    }
  }
}
