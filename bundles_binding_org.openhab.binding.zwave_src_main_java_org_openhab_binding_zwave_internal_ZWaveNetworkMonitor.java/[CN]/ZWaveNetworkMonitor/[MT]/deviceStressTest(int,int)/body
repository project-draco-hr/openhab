{
  ZWaveNode node=zController.getNode(nodeId);
  if (node == null)   return;
  ZWaveNoOperationCommandClass zwaveCommandClass=(ZWaveNoOperationCommandClass)node.getCommandClass(CommandClass.NO_OPERATION);
  if (zwaveCommandClass == null)   return;
  zController.sendData(zwaveCommandClass.getNoOperationMessage());
}
