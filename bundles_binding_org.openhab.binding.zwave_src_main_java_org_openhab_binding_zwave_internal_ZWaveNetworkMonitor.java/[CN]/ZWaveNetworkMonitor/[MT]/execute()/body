{
  if (networkHealNightlyTime > System.currentTimeMillis())   return;
  if (healNodes != null) {
    for (    Map.Entry<Integer,HealNode> entry : healNodes.entrySet()) {
      HealNode node=entry.getValue();
      if (node.state != HealState.WAITING) {
        nextHealStage(node);
        return;
      }
    }
    for (    Map.Entry<Integer,HealNode> entry : healNodes.entrySet()) {
      HealNode node=entry.getValue();
      if (node.battery == false) {
        nextHealStage(node);
        return;
      }
    }
  }
}
