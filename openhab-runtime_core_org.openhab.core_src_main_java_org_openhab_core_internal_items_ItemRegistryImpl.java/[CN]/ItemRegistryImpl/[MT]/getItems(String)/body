{
  String regex=pattern.replace("?",".?").replace("*",".*?");
  Collection<Item> matchedItems=new ArrayList<Item>();
  for (  Item[] items : itemMap.values()) {
    for (    Item item : items) {
      if (item.getName().matches(regex)) {
        matchedItems.add(item);
      }
    }
  }
  return matchedItems;
}
