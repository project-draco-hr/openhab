{
  long startTime=System.currentTimeMillis();
  try {
    ExtObjectContainer extObjectContainer=db.ext();
    System.err.println(extObjectContainer.version());
    db.commit();
    logger.debug("succesfully commited db4o transaction in {}ms",System.currentTimeMillis() - startTime);
  }
 catch (  Db4oException e) {
    db.rollback();
    logger.warn("Error commiting transaction : {}",e.getMessage());
  }
}
