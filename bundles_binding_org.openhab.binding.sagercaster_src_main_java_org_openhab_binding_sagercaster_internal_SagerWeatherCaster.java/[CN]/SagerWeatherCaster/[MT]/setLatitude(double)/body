{
  if (latitude >= 66.6) {
    usedDirections=NPZDIRECTIONS;
  }
 else   if (latitude >= 23.5) {
    usedDirections=NTZDIRECTIONS;
  }
 else   if (latitude >= 0) {
    usedDirections=NPZDIRECTIONS;
  }
 else   if (latitude > -23.5) {
    usedDirections=SPZDIRECTIONS;
  }
 else   if (latitude > -66.6) {
    usedDirections=STZDIRECTIONS;
  }
 else {
    usedDirections=SPZDIRECTIONS;
  }
}
