{
  if (state != null) {
    if (state.equals("ON") || state.equals("OPEN") || state.equals("UP")) {
      return setRightButtonState(true);
    }
 else     if (state.equals("OFF") || state.equals("CLOSED") || state.equals("DOWN")) {
      return setRightButtonState(false);
    }
  }
  return true;
}
