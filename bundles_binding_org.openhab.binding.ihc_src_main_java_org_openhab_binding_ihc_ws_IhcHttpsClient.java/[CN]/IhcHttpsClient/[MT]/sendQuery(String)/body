{
  try {
    return sendQ(query);
  }
 catch (  NoHttpResponseException e) {
    try {
      logger.debug("No response received, resend query");
      return sendQ(query);
    }
 catch (    IOException ee) {
      throw new IhcExecption(ee);
    }
  }
catch (  IOException e) {
    throw new IhcExecption(e);
  }
}
