{
  int value=serialMessage.getMessagePayloadByte(offset + 1);
  logger.debug(String.format("NODE %d: Thermostat Fan Mode report value = 0x%02X",this.getNode().getNodeId(),value));
  FanModeType fanModeType=FanModeType.getFanModeType(value);
  if (fanModeType == null) {
    logger.error(String.format("NODE %d: Unknown Fan Mode Type = 0x%02x, ignoring report.",this.getNode().getNodeId(),value));
    return;
  }
  if (!this.fanModeTypes.contains(fanModeType))   this.fanModeTypes.add(fanModeType);
  logger.debug("NODE {}: Thermostat Fan Mode Report value = {}",this.getNode().getNodeId(),fanModeType.getLabel());
  ZWaveCommandClassValueEvent zEvent=new ZWaveCommandClassValueEvent(this.getNode().getNodeId(),endpoint,this.getCommandClass(),new BigDecimal(value));
  this.getController().notifyEventListeners(zEvent);
}
