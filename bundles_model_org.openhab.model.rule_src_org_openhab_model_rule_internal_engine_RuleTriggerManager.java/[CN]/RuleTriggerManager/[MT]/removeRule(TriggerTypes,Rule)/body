{
switch (type) {
case STARTUP:
    systemStartupTriggeredRules.remove(rule);
  break;
case SHUTDOWN:
systemShutdownTriggeredRules.remove(rule);
break;
case UPDATE:
updateEventTriggeredRules.remove(rule);
break;
case CHANGE:
changedEventTriggeredRules.remove(rule);
break;
case COMMAND:
commandEventTriggeredRules.remove(rule);
break;
}
}
