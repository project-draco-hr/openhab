{
switch (type) {
case STARTUP:
    systemStartupTriggeredRules.clear();
  break;
case SHUTDOWN:
systemShutdownTriggeredRules.clear();
break;
case UPDATE:
updateEventTriggeredRules.clear();
break;
case CHANGE:
changedEventTriggeredRules.clear();
break;
case COMMAND:
commandEventTriggeredRules.clear();
break;
}
}
