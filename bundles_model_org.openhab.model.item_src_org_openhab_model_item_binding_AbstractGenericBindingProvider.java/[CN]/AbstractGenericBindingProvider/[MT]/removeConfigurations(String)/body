{
  Set<Item> items=null;
synchronized (contextMap) {
    items=contextMap.get(context);
    if (items != null) {
      contextMap.remove(context);
    }
  }
  if (items != null) {
    for (    Item item : items) {
      bindingConfigs.remove(item.getName());
      notifyListeners(item);
    }
  }
}
