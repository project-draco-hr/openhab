{
  Command command=null;
  if (valueObject.getValue().equals(Positions.DOWN.toString()))   command=new StringType("CLOSED");
 else   if (valueObject.getValue().equals(Positions.MIDDLE.toString()))   command=new StringType("OPEN");
 else   if (valueObject.getValue().equals(Positions.UP.toString()))   command=new StringType("AJAR");
  postCommand(command);
}
