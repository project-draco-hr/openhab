{
  this.restartTimeout=MAX_IDLE_BEFORE_RESTART;
  logger.info("Called with config " + config);
  if (config != null) {
    String maxIdle=(String)config.get("max_idle");
    if (maxIdle != null) {
      this.restartTimeout=Integer.valueOf(maxIdle);
    }
  }
  resetTellstick();
  setProperlyConfigured(true);
}
