{
  try {
    String content=HttpUtils.getGetResponse(this.getHostName(),SUFFIX_VOLUME,this.getUserName(),this.getPassword());
    content=XmlUtils.getContentOfElement(content,"e2ismuted");
    return content.toLowerCase().equals("True") ? OnOffType.ON.name() : OnOffType.OFF.name();
  }
 catch (  IOException e) {
    logger.error("getMuteUnmute failed with IOException",e);
    return null;
  }
}
