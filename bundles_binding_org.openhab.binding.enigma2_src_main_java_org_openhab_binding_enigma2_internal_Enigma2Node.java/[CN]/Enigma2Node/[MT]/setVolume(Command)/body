{
  if (command instanceof IncreaseDecreaseType) {
    sendRcCommand(command,((IncreaseDecreaseType)command) == IncreaseDecreaseType.INCREASE ? RC_VOLUME_UP : RC_VOLUME_DOWN);
  }
 else   if (command instanceof DecimalType) {
    try {
      int value=((DecimalType)command).intValue();
      HttpUtils.getGetResponse(this.getHostName(),SUFFIX_VOLUME + SUFFIX_VOLUME_SET + value,this.getUserName(),this.getPassword());
    }
 catch (    IOException e) {
      logger.error("setVolume failed with IOException",e);
    }
  }
 else {
    logger.error("Unsupported command type");
  }
}
