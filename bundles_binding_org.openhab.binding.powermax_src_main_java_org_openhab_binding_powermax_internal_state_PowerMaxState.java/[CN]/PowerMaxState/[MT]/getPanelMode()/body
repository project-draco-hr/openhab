{
  String mode=null;
  if ((downloadMode != null) && downloadMode.equals(Boolean.TRUE)) {
    mode="Download";
  }
 else   if ((powerlinkMode != null) && powerlinkMode.equals(Boolean.TRUE)) {
    mode="Powerlink";
  }
 else   if ((powerlinkMode != null) && powerlinkMode.equals(Boolean.FALSE)) {
    mode="Standard";
  }
  return mode;
}
