{
  super();
  this.item=item;
  this.command=command;
  this.remote=remote;
  this.direction=direction;
  this.isBlocking=isBlocking;
  this.buffer=buffer;
  this.isReconnecting=isReconnecting;
  this.channel=channel;
  this.host=remote.getHostString();
  this.port=Integer.toString(remote.getPort());
}
