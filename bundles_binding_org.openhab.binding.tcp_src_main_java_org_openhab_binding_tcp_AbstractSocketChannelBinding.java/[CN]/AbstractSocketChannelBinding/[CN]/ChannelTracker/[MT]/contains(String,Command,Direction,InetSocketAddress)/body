{
synchronized (this) {
    Iterator<C> it=iterator();
    while (it.hasNext()) {
      C aChannel=it.next();
      if (item.equals(aChannel.item) && command.equals(aChannel.command) && direction.equals(aChannel.direction)&& remote.equals(aChannel.remote)) {
        return true;
      }
    }
    return false;
  }
}
