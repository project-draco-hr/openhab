{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  ZWaveDeviceClass other=(ZWaveDeviceClass)obj;
  if (basicDeviceClass != other.basicDeviceClass)   return false;
  if (genericDeviceClass != other.genericDeviceClass)   return false;
  if (specificDeviceClass != other.specificDeviceClass)   return false;
  return true;
}
