{
  if (device == null && !StringUtils.isNumeric(configParts[0].trim())) {
    throw new BindingConfigParseException("item '" + item.getName() + "' telldus device "+ configParts[0].trim()+ " not found");
  }
  if (configParts.length > 3 && !StringUtils.isNumeric(configParts[3].trim())) {
    throw new BindingConfigParseException("item '" + item.getName() + "' resend config wrong"+ configParts[3].trim()+ " not a number");
  }
}
