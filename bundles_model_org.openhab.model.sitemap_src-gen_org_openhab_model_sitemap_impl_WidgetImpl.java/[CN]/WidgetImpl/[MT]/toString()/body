{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (label: ");
  result.append(label);
  result.append(", icon: ");
  result.append(icon);
  result.append(", item: ");
  result.append(item);
  result.append(')');
  return result.toString();
}
