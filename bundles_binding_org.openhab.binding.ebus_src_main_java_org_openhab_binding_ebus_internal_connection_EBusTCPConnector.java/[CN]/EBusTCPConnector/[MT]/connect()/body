{
  try {
    socket=new Socket(hostname,port);
    socket.setSoTimeout(20000);
    socket.setKeepAlive(true);
    socket.setTcpNoDelay(true);
    inputStream=socket.getInputStream();
    outputStream=socket.getOutputStream();
    return true;
  }
 catch (  Exception e) {
    logger.error(e.toString(),e);
  }
  return false;
}
