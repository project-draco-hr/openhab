{
  if ((groupID == -1 && !force)) {
    return;
  }
  if (device.isDimmable()) {
switch (sceneID) {
case 11:
case 42:
case 44:
case 46:
case 48:
      decrease(device);
    break;
case 12:
case 43:
case 45:
case 47:
case 49:
  increase(device);
break;
default :
break;
}
}
 else if (device.isRollershutter()) {
switch (sceneID) {
case 15:
initOutputValue(device,DeviceConstants.DEVICE_SENSOR_OUTPUT);
if (device.getOutputMode().equals(OutputModeEnum.SLAT)) {
initOutputValue(device,DeviceConstants.DEVICE_SENSOR_SLAT_OUTPUT);
}
break;
case 52:
case 53:
case 54:
case 55:
initOutputValue(device,DeviceConstants.DEVICE_SENSOR_OUTPUT);
if (device.getOutputMode().equals(OutputModeEnum.SLAT)) {
initOutputValue(device,DeviceConstants.DEVICE_SENSOR_SLAT_OUTPUT);
}
break;
default :
}
}
}
