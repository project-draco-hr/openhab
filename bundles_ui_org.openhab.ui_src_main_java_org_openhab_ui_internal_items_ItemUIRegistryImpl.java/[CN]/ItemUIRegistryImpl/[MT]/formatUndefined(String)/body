{
  String undefinedFormatPattern=formatPattern.replaceAll(IDENTIFY_FORMAT_PATTERN_PATTERN,"%1\\$s");
  try {
    return String.format(undefinedFormatPattern,"-");
  }
 catch (  Exception e) {
    logger.warn("Exception while formatting undefined value [sourcePattern={}, targetPattern={}, {}]",formatPattern,undefinedFormatPattern,e);
    return "Err";
  }
}
