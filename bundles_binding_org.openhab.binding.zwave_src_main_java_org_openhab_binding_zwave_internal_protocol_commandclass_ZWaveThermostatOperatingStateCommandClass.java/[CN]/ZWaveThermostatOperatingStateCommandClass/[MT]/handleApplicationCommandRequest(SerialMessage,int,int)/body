{
  logger.debug("NODE {}: Received Thermostat Operating State Request",this.getNode().getNodeId());
  int command=serialMessage.getMessagePayloadByte(offset);
switch (command) {
case THERMOSTAT_OPERATING_STATE_REPORT:
    logger.trace("NODE {}: Process Thermostat Operating State Report",this.getNode().getNodeId());
  processThermostatOperatingStateReport(serialMessage,offset,endpoint);
if (this.getNode().getNodeStage() != NodeStage.DONE) this.getNode().advanceNodeStage(NodeStage.DONE);
break;
default :
logger.warn(String.format("NODE %d: Unsupported Command 0x%02X for command class %s (0x%02X).",this.getNode().getNodeId(),command,this.getCommandClass().getLabel(),this.getCommandClass().getKey()));
}
}
