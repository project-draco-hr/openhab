{
  if (state instanceof PercentType && state.equals(new PercentType(0))) {
    return true;
  }
 else   if (state instanceof OnOffType && state.equals(OnOffType.OFF)) {
    return true;
  }
 else   if (state instanceof OpenClosedType && state.equals(OpenClosedType.CLOSED)) {
    return true;
  }
  return false;
}
