{
switch (connectorType) {
case IT100:
    if (api == null) {
      api=new API(serialPort,baudRate,userCode);
    }
  break;
case ENVISALINK:
if (api == null) {
  api=new API(ipAddress,tcpPort,password,userCode);
}
break;
default :
connected=false;
logger.debug("openConnection(): Unable to make a connection!");
return;
}
connected=api.open();
if (connected) {
api.addEventListener(this);
}
}
