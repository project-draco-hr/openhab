{
  String itemName="";
  itemName=getItemName(dscAlarmItemType,partitionId,zoneId);
  logger.debug("updateTriggerItem(): Item Name: {} Partition: {} Zone: {}",itemName,partitionId,zoneId);
  if (itemName != "") {
    updateDeviceProperties(itemName,propertyState,"");
    updateItem(itemName);
  }
}
