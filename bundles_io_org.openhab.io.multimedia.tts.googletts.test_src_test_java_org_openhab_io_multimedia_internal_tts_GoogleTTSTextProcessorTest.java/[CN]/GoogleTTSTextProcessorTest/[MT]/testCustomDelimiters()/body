{
  String customDelimiters="??????";
  textProcessor.setCustomSentenceDelimiters(customDelimiters);
  String sentence="??????????????????????????????";
  List<String> splitSentences=textProcessor.splitIntoChunks(sentence);
  Assert.assertEquals(2,splitSentences.size());
