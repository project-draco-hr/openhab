{
  if (baseClass == GenericDevice.class) {
switch (baseFeatureID) {
case ModelPackage.GENERIC_DEVICE__GENERIC_DEVICE_ID:
      return ModelPackage.DIGITAL_SENSOR__GENERIC_DEVICE_ID;
default :
