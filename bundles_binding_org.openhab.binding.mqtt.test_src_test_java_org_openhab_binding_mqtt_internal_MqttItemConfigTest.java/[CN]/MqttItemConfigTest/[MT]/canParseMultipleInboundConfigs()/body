{
  MqttItemConfig c=new MqttItemConfig("myItem","<[mybroker:/myHome/doorbell:state:XSLT(doorbell.xslt)], <[mybroker:/myHome/doorbell:command:ON], <[mybroker:/myHome/doorbell:state:XSLT(doorbell.xslt)]");
  assertEquals(0,c.getMessagePublishers().size());
  assertEquals(3,c.getMessageSubscribers().size());
}
