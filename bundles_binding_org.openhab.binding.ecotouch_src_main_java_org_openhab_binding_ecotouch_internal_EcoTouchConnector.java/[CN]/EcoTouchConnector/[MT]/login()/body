{
  cookies=null;
  try {
    String url="http://" + ip + "/cgi/login?username="+ URLEncoder.encode(username,"UTF-8")+ "&password="+ URLEncoder.encode(password,"UTF-8");
    URL loginurl=new URL(url);
    URLConnection connection=loginurl.openConnection();
    cookies=connection.getHeaderFields().get("Set-Cookie");
  }
 catch (  Exception e) {
    logger.debug("Cannot log into Waterkotte EcoTouch.");
  }
}
