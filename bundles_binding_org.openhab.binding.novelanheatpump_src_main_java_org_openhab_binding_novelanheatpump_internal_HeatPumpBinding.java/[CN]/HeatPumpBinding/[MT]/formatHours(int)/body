{
  String returnValue="";
  returnValue+=String.format("%02d:",new Object[]{Integer.valueOf(value / 3600)});
  value%=3600;
  returnValue+=String.format("%02d:",new Object[]{Integer.valueOf(value / 60)});
  value%=60;
  returnValue+=String.format("%02d",new Object[]{Integer.valueOf(value)});
  return returnValue;
}
