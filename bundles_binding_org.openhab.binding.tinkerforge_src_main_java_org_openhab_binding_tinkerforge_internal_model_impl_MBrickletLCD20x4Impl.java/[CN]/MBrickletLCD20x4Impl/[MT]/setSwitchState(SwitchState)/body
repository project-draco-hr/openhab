{
  SwitchState oldSwitchState=switchState;
  switchState=newSwitchState == null ? SWITCH_STATE_EDEFAULT : newSwitchState;
  try {
    if (switchState == SwitchState.OFF) {
      logger.debug("setSwitchState off");
      tinkerforgeDevice.backlightOff();
    }
 else     if (switchState == SwitchState.ON) {
      logger.debug("setSwitchState on");
      tinkerforgeDevice.backlightOn();
    }
 else     logger.error("unkown switchstate");
  }
 catch (  TimeoutException e) {
    TinkerforgeErrorHandler.handleError(this,TinkerforgeErrorHandler.TF_TIMEOUT_EXCEPTION,e);
  }
catch (  NotConnectedException e) {
    TinkerforgeErrorHandler.handleError(this,TinkerforgeErrorHandler.TF_NOT_CONNECTION_EXCEPTION,e);
  }
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,ModelPackage.MBRICKLET_LCD2_0X4__SWITCH_STATE,oldSwitchState,switchState));
}
