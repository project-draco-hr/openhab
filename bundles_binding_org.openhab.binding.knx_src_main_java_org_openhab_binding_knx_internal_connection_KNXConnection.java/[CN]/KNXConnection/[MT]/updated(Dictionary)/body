{
  if (config != null) {
    ip=(String)config.get("ip");
    if (ip == null) {
      port=(String)config.get("port");
    }
    String readingPauseString=(String)config.get("pause");
    if (readingPauseString != null && !readingPauseString.isEmpty()) {
      readingPause=Long.parseLong(readingPauseString);
    }
    if (pc == null)     connect();
  }
}
