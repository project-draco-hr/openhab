{
  if (baseClass == MSensor.class) {
switch (baseFeatureID) {
case ModelPackage.MSENSOR__SENSOR_VALUE:
      return ModelPackage.MBRICKLET_TEMPERATURE__SENSOR_VALUE;
case ModelPackage.MSENSOR__CALLBACK_PERIOD:
    return ModelPackage.MBRICKLET_TEMPERATURE__CALLBACK_PERIOD;
default :
  return -1;
}
}
if (baseClass == MTFConfigConsumer.class) {
switch (baseFeatureID) {
case ModelPackage.MTF_CONFIG_CONSUMER__TF_CONFIG:
return ModelPackage.MBRICKLET_TEMPERATURE__TF_CONFIG;
default :
return -1;
}
}
return super.eDerivedStructuralFeatureID(baseFeatureID,baseClass);
}
