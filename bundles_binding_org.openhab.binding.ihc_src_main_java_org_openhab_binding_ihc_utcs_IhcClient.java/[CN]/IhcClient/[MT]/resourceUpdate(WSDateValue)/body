{
  String soapQuery="<?xml version=\"1.0\" encoding=\"UTF-8\"?>" + "<soap:Envelope xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema\" xmlns:soap=\"http://schemas.xmlsoap.org/soap/envelope/\">" + "<soap:Body>"+ " <setResourceValue1 xmlns=\"utcs\">"+ "  <value xmlns:q1=\"utcs.values\" xsi:type=\"q1:WSDateValue\">"+ "   <q1:month>" + value.getMonth() + "</q1:month>"+ "   <q1:year>"+ value.getYear()+ "</q1:year>"+ "   <q1:day>"+ value.getDay()+ "</q1:day>"+ "  </value>"+ "  <resourceID>"+ value.getResourceID()+ "</resourceID>"+ "  <isValueRuntime>true</isValueRuntime>"+ " </setResourceValue1>"+ "</soap:Body>"+ "</soap:Envelope>";
