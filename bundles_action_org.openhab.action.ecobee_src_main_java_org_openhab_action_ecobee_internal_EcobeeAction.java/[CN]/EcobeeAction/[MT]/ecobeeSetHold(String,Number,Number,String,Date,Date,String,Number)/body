{
  SetHoldFunction function=new SetHoldFunction((coolHoldTemp == null) ? null : Temperature.fromLocalTemperature(new BigDecimal(coolHoldTemp.toString())),(heatHoldTemp == null) ? null : Temperature.fromLocalTemperature(new BigDecimal(heatHoldTemp.toString())),holdClimateRef,startDateTime,endDateTime,(holdType == null) ? null : HoldType.forValue(holdType),(holdHours == null) ? null : holdHours.intValue());
  return callEcobeeInternal(selection,function);
}
