{
  if (selManufacturer == null)   return false;
  for (  ZWaveDbProduct product : selManufacturer.Product) {
    for (    ZWaveDbProductReference reference : product.Reference) {
      if (reference.Type == productType && reference.Id == productId) {
        selProduct=product;
        return true;
      }
    }
  }
  return false;
}
