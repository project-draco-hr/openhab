{
  if (database == null)   return false;
  selManufacturer=null;
  selProduct=null;
  productFile=null;
  for (  ZWaveDbManufacturer manufacturer : database.Manufacturer) {
    if (manufacturer.Id == manufacturerId) {
      selManufacturer=manufacturer;
      return true;
    }
  }
  return false;
}
