{
  SunCalc sunCalc=new SunCalc();
  Sun sun=sunCalc.getSunInfo(Calendar.getInstance(),context.getConfig().getLatitude(),context.getConfig().getLongitude());
  SunPosition sp=sunCalc.getSunPosition(Calendar.getInstance(),context.getConfig().getLatitude(),context.getConfig().getLongitude());
  sun.setPosition(sp);
  context.setPlanet(PlanetName.SUN,sun);
  planetPublisher.publish(PlanetName.SUN);
}
