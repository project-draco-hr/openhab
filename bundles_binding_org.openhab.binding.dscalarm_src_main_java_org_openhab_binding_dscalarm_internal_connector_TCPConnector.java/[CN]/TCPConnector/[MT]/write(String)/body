{
  try {
    tcpOutput.write(writeString);
    tcpOutput.flush();
  }
 catch (  IOException ioException) {
    logger.error("write(): {}",ioException);
    connected=false;
  }
catch (  Exception exception) {
    logger.error("write(): Unable to write to socket: {} ",exception);
    connected=false;
  }
}
