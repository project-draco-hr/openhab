{
  String retVal="";
  int pos, pos2, length;
  if (0 < (pos=msg.indexOf(parameter))) {
    length=parameter.length();
    pos2=msg.indexOf(" ",pos + length);
    if (pos2 > 0) {
      retVal=msg.substring(pos + length,msg.lastIndexOf(" ",pos2));
    }
 else {
      retVal=msg.substring(pos + length);
    }
  }
  if (retVal.equals("0")) {
    retVal="";
  }
  return retVal;
}
