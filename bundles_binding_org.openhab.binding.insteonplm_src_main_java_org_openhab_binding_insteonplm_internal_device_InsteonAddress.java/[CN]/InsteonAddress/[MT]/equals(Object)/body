{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  InsteonAddress other=(InsteonAddress)obj;
  if (highByte != other.highByte)   return false;
  if (lowByte != other.lowByte)   return false;
  if (middleByte != other.middleByte)   return false;
  if (x10 != other.x10)   return false;
  return true;
}
