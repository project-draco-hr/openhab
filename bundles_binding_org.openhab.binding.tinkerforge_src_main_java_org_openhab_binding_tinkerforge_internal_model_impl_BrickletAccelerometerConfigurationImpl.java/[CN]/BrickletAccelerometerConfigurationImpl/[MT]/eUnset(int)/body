{
switch (featureID) {
case ModelPackage.BRICKLET_ACCELEROMETER_CONFIGURATION__DATA_RATE:
    setDataRate(DATA_RATE_EDEFAULT);
  return;
case ModelPackage.BRICKLET_ACCELEROMETER_CONFIGURATION__FULL_SCALE:
