{
  if (args.length == 0) {
    System.err.println("Aborting: no path to EMF resource provided!");
    return;
  }
  Injector injector=new org.openhab.model.rule.RulesStandaloneSetupGenerated().createInjectorAndDoEMFRegistration();
  Main main=injector.getInstance(Main.class);
  main.runGenerator(args[0]);
}
