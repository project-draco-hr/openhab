{
  Reader reader=null;
  this.logger.debug("Do login for user '{}'",this.userName);
  try {
    HttpURLConnection httpURLConnection;
    httpURLConnection=(HttpURLConnection)new URL(API_URL + "auth?email=" + this.userName+ "&password="+ this.password).openConnection();
    InputStream inputStream=httpURLConnection.getInputStream();
    reader=new InputStreamReader(inputStream,"UTF-8");
    JsonObject jsonObject=(JsonObject)jsonParser.parse(reader);
    String status=jsonObject.get("status").getAsString();
    if (!status.equals("ok")) {
      return false;
    }
    this.logger.debug("Logon successfull");
    this.authToken=jsonObject.get("authToken").getAsString();
    return true;
  }
 catch (  Exception ex) {
    this.logger.error("Error do logon: '{}'",ex.toString());
    return false;
  }
 finally {
    if (reader != null) {
      try {
        reader.close();
      }
 catch (      IOException ignored) {
      }
    }
  }
}
