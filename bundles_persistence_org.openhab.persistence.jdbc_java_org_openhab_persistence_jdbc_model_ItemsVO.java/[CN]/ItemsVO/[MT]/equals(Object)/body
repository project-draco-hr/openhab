{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  ItemsVO other=(ItemsVO)obj;
  if (itemname == null) {
    if (other.itemname != null) {
      return false;
    }
  }
 else   if (!itemname.equals(other.itemname)) {
    return false;
  }
  if (itemid != other.itemid) {
    return false;
  }
  return true;
}
