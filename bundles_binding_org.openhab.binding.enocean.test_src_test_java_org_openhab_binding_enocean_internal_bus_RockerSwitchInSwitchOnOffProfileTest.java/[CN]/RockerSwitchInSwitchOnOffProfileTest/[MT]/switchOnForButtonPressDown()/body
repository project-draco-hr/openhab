{
  EnoceanParameterAddress valueParameterAddress=new EnoceanParameterAddress(EnoceanId.fromString(EnoceanBindingProviderMock.DEVICE_ID),CHANNEL,RockerSwitch.BUTTON_I);
  binding.valueChanged(valueParameterAddress,ButtonState.PRESSED);
  assertEquals("Update State",OnOffType.ON,publisher.popLastCommand());
}
