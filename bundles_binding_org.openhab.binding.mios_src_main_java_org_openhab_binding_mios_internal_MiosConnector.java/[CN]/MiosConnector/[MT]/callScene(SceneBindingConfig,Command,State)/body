{
  logger.debug("callScene: Need to remote-invoke Scene '{}'",config.toProperty());
  String newCommand=config.transformCommand(command);
  if (newCommand != null) {
    MiosUnit u=getMiosUnit();
    callMios(String.format(SCENE_URL,u.getHostname(),u.getPort(),config.getId()));
  }
 else {
    logger.debug("invokeScene: Command type not supported for Scenes '{}'",command.getClass());
  }
}
