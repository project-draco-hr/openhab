{
  if (baseClass == MSensor.class) {
switch (baseFeatureID) {
case ModelPackage.MSENSOR__SENSOR_VALUE:
      return ModelPackage.JOYSTICK_BUTTON__SENSOR_VALUE;
default :
    return -1;
}
}
