{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  InsteonHubBindingDeviceInfo other=(InsteonHubBindingDeviceInfo)obj;
  if (deviceId == null) {
    if (other.deviceId != null)     return false;
  }
 else   if (!deviceId.equals(other.deviceId))   return false;
  if (hubId == null) {
    if (other.hubId != null)     return false;
  }
 else   if (!hubId.equals(other.hubId))   return false;
  return true;
}
