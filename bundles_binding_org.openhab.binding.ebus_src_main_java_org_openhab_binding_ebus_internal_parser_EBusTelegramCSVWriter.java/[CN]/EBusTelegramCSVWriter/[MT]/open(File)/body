{
  fw=new FileWriter(csvFile);
  writer=new BufferedWriter(fw);
  if (!csvFile.exists()) {
    csvFile.createNewFile();
  }
  writer.write("Date/Time;");
  writer.write("TYPE;");
  writer.write("SRC;");
  writer.write("DST;");
  writer.write("CMD;");
  writer.write("LEN;");
  writer.write("DATA;");
  writer.write("CRC;");
  writer.write("ACK;");
  writer.write("S LEN;");
  writer.write("S DATA;");
  writer.write("S CRC;");
  writer.write("COMMENT");
  writer.newLine();
  writer.flush();
}
