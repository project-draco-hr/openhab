{
  int state=0;
switch (stateType) {
case CONNECTION_STATE:
    state=systemConnection;
  break;
case GENERAL_STATE:
state=generalState;
break;
case ARM_STATE:
state=armState;
break;
case ALARM_STATE:
state=alarmState;
break;
case TAMPER_STATE:
state=tamperState;
break;
case FAULT_STATE:
state=faultState;
break;
default :
break;
}
return state;
}
