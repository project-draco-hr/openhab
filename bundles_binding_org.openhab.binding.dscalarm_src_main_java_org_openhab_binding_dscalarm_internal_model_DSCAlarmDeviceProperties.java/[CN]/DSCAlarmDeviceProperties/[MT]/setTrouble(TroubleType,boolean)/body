{
switch (troubleType) {
case SERVICE_REQUIRED:
    serviceRequired=trouble;
  break;
case AC_TROUBLE:
acTrouble=trouble;
break;
case TELEPHONE_LINE_TROUBLE:
telephoneLineTrouble=trouble;
break;
case FAILURE_TO_COMMUNICATE:
failureToCommunicate=trouble;
break;
case ZONE_FAULT:
zoneFault=trouble;
break;
case ZONE_TAMPER:
zoneTamper=trouble;
break;
case ZONE_LOW_BATTERY:
zoneLowBattery=trouble;
break;
case LOSS_OF_TIME:
lossOfTime=trouble;
break;
default :
break;
}
}
