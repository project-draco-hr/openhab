{
  request.setUnitID(getId());
  transaction.setRequest(request);
  try {
    transaction.execute();
  }
 catch (  Exception e) {
    logger.debug("ModbusSlave:" + e.getMessage());
    return null;
  }
  ModbusResponse r=transaction.getResponse();
  if ((r.getTransactionID() != transaction.getTransactionID()) && !r.isHeadless()) {
    return null;
  }
  return r;
}
